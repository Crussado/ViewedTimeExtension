{
    "name": "Viewed Time",
    "description": "Measures time watching anime among others.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}
